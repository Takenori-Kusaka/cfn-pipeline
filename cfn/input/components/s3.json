{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "s3.json",
  "Parameters": {
    "BucketName": {
      "Type": "String",
      "Default": "BucketName"
    }
  },
  "Resources": {
    "S3Bucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": { "Ref" : "BucketName" }
      }
    }
  }
}
